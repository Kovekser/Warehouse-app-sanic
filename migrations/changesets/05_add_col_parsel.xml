<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        logicalFilePath="changesets/addColParsel.xml"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="kkovale@softserveinc.com" id="add_col_parsel">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists schemaName="public" tableName="parsel"/>
            </not>
        </preConditions>
        <addColumn schemaName="public" tableName="parsel">
            <column name="volume" type="decimal"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>